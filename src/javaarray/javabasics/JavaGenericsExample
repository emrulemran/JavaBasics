package genericsdemo;

public class JavaGenricsExample {

	public static void main(String[] args) {

		Integer[] iA = { 1, 2, 3, 4 };

		Character[] cA = { 'a', 'b', 'c', 'd' };
		printMe(iA);
		printMe(cA);
	}

	public static <T> void printMe(T[] i) {

		for (T x : i) {
			System.out.printf("%s ", x);

		}
		System.out.println();
	}

}
